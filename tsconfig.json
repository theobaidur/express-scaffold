{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "experimentalDecorators": true,
  },
  "include": ["src/**/*"]
}
